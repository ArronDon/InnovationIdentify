<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.innovation.identity.activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />
    <!-- INTENET权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 查看手机网络状态 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 获取照相机权限 -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- 获取SD卡读写权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.innovation.identity.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.innovation.identity.activity.T1Activity" >
        </activity>
        <activity android:name="com.innovation.identity.activity.T2Activity" >
            
        </activity>
        <activity android:name="com.innovation.identity.activity.T3Activity" >
        </activity>
        <activity android:name="com.innovation.identity.loginregister.activity.LoginActivity" >
        </activity>
        <activity
            android:name="com.innovation.identity.activity.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
    </application>

</manifest>
